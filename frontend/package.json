  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.production.json && rollup -c && eleventy",
    "clean": "rm -rf _site",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
